{
   "name": "svelte-starter-postcss",
   "version": "1.0.0",
   "description": "A starter for bulding Svelte web apps, with PostCSS support.",
   "main": " ",
   "scripts": {
      "build": "rollup -c",
      "build:auto": "rollup -c -w",
      "serve": "sirv dist",
